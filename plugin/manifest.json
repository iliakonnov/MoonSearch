{
    "manifest_version": 2,
    "name": "Moonwalk opener",
    "description": "This extension search kinopoisk id on moonwalk",
    "version": "1.0",
    "permissions": [
        "webRequest", "tabs", "*://kinopoisk.ru/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.kinopoisk.ru/*"],
            "css": ["css.css"],
            "js": ["jquery.js", "script.js"]
        }
    ]
}
